{
    "name": "osmphp/app",
    "description": "User/permission management and other common backend/frontend modules",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Osm\\App\\": "src/",
            "Osm\\App\\Tests\\": "tests/",
            "Osm\\App\\Samples\\": "samples/"
        }
    },
    "require": {
        "php": ">=7.3",
        "osmphp/framework2": "^0.1",
        "osmphp/data": "^0.1",
        "osmphp/bootstrap": "^0.1"
    },
    "extra": {
        "osm": {
            "component_pools": {
                "src": {
                    "module_path": "*/Module.php",
                    "theme_path": "*/theme.php"
                },
                "samples": {
                    "module_path": "*/Module.php",
                    "testing": true
                }
            }
        }
    }
}